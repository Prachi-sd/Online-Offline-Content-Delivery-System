<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/iexp">

    <TextView
        android:text="Roll No."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="174dp"
        android:id="@+id/textView65"
        android:textColor="#FFFFFF"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:id="@+id/textView67"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:textColor="#FFFFFF"
        android:layout_below="@+id/textView65"
        android:layout_alignLeft="@+id/textView65"
        android:layout_alignStart="@+id/textView65" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_alignTop="@+id/textView65"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText35"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:layout_alignBottom="@+id/textView67"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/editText36"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <Button
        android:text="LOGIN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button14"
        android:layout_below="@+id/editText36"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="53dp"
        style="@style/Widget.AppCompat.Button"
        android:background="?android:attr/detailsElementBackground"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:id="@+id/textView68"
        android:textColor="#FFFFFF"
        android:text="3"
        android:layout_below="@+id/button14"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />
</RelativeLayout>